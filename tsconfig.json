{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2019",
      "dom",
      "dom.iterable"
    ],
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "incremental": true,
    "outDir": "./.ts-cache/",
    "strictNullChecks": true
  },
  "include": [
    "./src/**/*"
  ]
}